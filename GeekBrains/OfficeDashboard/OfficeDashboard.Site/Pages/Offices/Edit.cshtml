@page
@model OfficeDashboard.Site.Pages.Offices.EditModel

<form method="post">
	<div asp-validation-summary="ModelOnly" class="text-danger"></div>
	<input type="hidden" asp-for="Office.Id"/>

	<div>
		<label asp-for="Office.Name"></label>
		<input type="text" asp-for="Office.Name"/>
		<span asp-validation-for="Office.Name" class="text-danger"></span>
	</div>
	<div>
		<button type="submit" asp-page-handler="GoBack">GoBack</button>
		<button type="submit">Save</button>
	</div>
</form>


@section Scripts {
	@{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}